{
  "manifest_version": 3,
  "name": "Youtube-focus",
  "version": "1.0.0",
  "permissions": ["scripting", "tabs"],
  "commands": {
    "focus": {
      "suggested_key": {
        "default": "Alt+Shift+F"
      },
      "description": "Focus on YouTube video only"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://www.youtube.com/*"]
}
